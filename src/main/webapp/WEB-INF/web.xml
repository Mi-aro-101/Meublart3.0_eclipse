<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" version="6.0">
  <servlet>
    <servlet-name>FrontServlet</servlet-name>
    <servlet-class>org.etu2020.servlet.FrontServlet</servlet-class>
    <init-param>
      <param-name>root</param-name>
      <param-value>org.meublart.controller</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>FrontServlet</servlet-name>
    <url-pattern>*.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>StyleServlet</servlet-name>
    <servlet-class>org.etu2020.servlet.StyleServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StyleServlet</servlet-name>
    <url-pattern>/StyleServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>MeubleParamServlet</display-name>
    <servlet-name>MeubleParamServlet</servlet-name>
    <servlet-class>org.etu2020.servlet.MeubleParamServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MeubleParamServlet</servlet-name>
    <url-pattern>/MeubleParamServlet</url-pattern>
  </servlet-mapping>
</web-app>